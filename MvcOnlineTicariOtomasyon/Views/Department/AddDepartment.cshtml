@model MvcOnlineTicariOtomasyon.Models.Classes.Department
@{
    ViewBag.Title = "AddDepartment";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br/>
<h2>Departman Ekle</h2>
<br/>
<form class="form-group" method="post">
    @Html.LabelFor(x => x.DepartmentName)
    @Html.TextBoxFor(x => x.DepartmentName, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Status)
    @Html.TextBoxFor(x => x.Status, new { @class = "form-control" })
    <br/>
    <button class="btn btn-primary">Kaydet</button>
</form>

